{
  "name": "cf-ai-cybersecurity-threat-intelligence",
  "version": "1.0.0",
  "description": "AI-Powered Cybersecurity Threat Intelligence Platform built on Cloudflare",
  "main": "src/workers/main.js",
  "scripts": {
    "dev": "wrangler dev --local",
    "dev:remote": "wrangler dev",
    "deploy": "wrangler deploy",
    "deploy:pages": "wrangler pages deploy src/frontend",
    "deploy:all": "wrangler deploy && wrangler pages deploy src/frontend",
    "build": "npm run build:workers && npm run build:frontend",
    "build:workers": "webpack --mode production",
    "build:frontend": "cd src/frontend && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "type-check": "tsc --noEmit",
    "login": "wrangler login",
    "whoami": "wrangler whoami",
    "tail": "wrangler tail"
  },
  "keywords": [
    "cybersecurity",
    "threat-intelligence",
    "ai",
    "cloudflare",
    "workers",
    "llama",
    "security"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231218.0",
    "@types/jest": "^29.5.8",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-environment-miniflare": "^2.14.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "wrangler": "^3.19.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/cf_ai_cybersecurity_threat_intelligence.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/cf_ai_cybersecurity_threat_intelligence/issues"
  },
  "homepage": "https://github.com/yourusername/cf_ai_cybersecurity_threat_intelligence#readme"
}
